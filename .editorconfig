
[*.{asax,ascx,aspx,cshtml,css,htm,html,js,json,jsx,master,proto,razor,resjson,skin,ts,tsx}]
indent_style=tab
indent_size=tab
tab_width=4

[*.{cs,vb,xaml,xamlx,xoml}]
indent_style=space
indent_size=4
tab_width=4

[*.{appxmanifest,axml,build,config,csproj,dbml,discomap,dtd,jsproj,lsproj,njsproj,nuspec,proj,props,resw,resx,StyleCop,targets,tasks,vbproj,xml,xsd}]
indent_style=space
